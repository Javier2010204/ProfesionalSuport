<p id="notice"><%= notice %></p>

<h1>Contracts</h1>

<table>
  <thead>
    <tr>
      <th>Place</th>
      <th>Name</th>
      <th>Nacionality</th>
      <th>Profession</th>
      <th>Academy degree</th>
      <th>University</th>
      <th>Departament</th>
      <th>Dpi</th>
      <th>Age</th>
      <th>Marital status</th>
      <th>Email</th>
      <th>Phone</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @contracts.each do |contract| %>
      <tr>
        <td><%= contract.place %></td>
        <td><%= contract.name %></td>
        <td><%= contract.nacionality %></td>
        <td><%= contract.profession %></td>
        <td><%= contract.academy_degree %></td>
        <td><%= contract.university %></td>
        <td><%= contract.departament %></td>
        <td><%= contract.dpi %></td>
        <td><%= contract.age %></td>
        <td><%= contract.marital_status %></td>
        <td><%= contract.email %></td>
        <td><%= contract.phone %></td>
        <td><%= link_to 'Show', contract %></td>
        <td><%= link_to 'Edit', edit_contract_path(contract) %></td>
        <td><%= link_to 'Destroy', contract, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Contract', new_contract_path %>
